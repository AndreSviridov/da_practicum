    "query_3 = '''SELECT p.publisher AS publisher,\n",
    "                    count(b.book_id) AS books_qty\n",
    "             FROM books AS b\n",
    "             JOIN publishers AS p on b.publisher_id=p.publisher_id\n",
    "             WHERE b.num_pages > 50\n",
    "             GROUP BY publisher\n",
    "             ORDER BY books_qty DESC\n",
    "             LIMIT 1'''\n",
